{
    "name" : "Meme Tab",
    "version" : "1.0",
    "manifest_version" : 3,
    "description" : "a meme a tab",
    "icons" : {"128" : "icons/icon.png"},
    "chrome_url_overrides" : {
        "newtab": "memetab.html"
    },
    "web_accessible_resources": [{
        "resources" : [ "memes/*", "icons/*" ],
        "matches": ["<all_urls>"]
    }]
}