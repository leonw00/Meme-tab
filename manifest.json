{
    "name" : "Meme Tab",
    "version" : "1.0",
    "manifest_version" : 2,
    "chrome_url_overrides" : {
        "newtab": "memetab.html"
    },
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "web_accessible_resources": [
        "memes/*"
    ]
}